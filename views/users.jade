extends layout

block content
    div.navbar.navbar-fixed-top
        div.container-fluid
            // Wrapper
            #wrapper.row
                div.col-sm-3.col-md-2.sidebar
                    h1= "Menu"
                    ul.nav
                        li
                            a(href="/account/getTestlist/all")#getTests="Tests"
                        li
                            a(href="/users/all")#getUsers= "Users"
                        li
                            a= "Results"
                        li
                            a= "Pictures"
                div.col-sm-9.col-sm-offset-3.col-md-10.col-md-offset-2.main
                    // USER LIST
                    h1.page-header= "Users List"
                    form.navbar-form.navbar-right(action="/account/signup/admin" method="POST")
                        input(type="text", placeholder="Username" name="fullName").form-control
                        input(type="email", placeholder="Email" name="email").form-control
                        input(type="password", placeholder="password" name="password").form-control
                        input(type="submit", value="Add User").form-control.btn-primary
                    h2 Users List
                    each result, key in data
                        form.navbar-form.navbar-left(action="/account/delTest" method="DELETE")
                             each value, key in result
                                  input(type="text" name="#{key}" value="#{value}").form-control
                             input(type="submit" value="Delete").btn.btn-primary
        // /USER LIST

    // /WRAPPER
